                        <!-- about us text -->
                        <section class="s_title pt40 pb40 o_colored_level" data-vcss="001" data-snippet="s_title" data-name="Title">
                            <div class="container s_allow_columns">
                                <h2 class="text-center"><strong>
                                    <font>See What our Customers</font></strong>
                                </h2>
                                <h2 class="text-center"><strong>
                                    <font>&amp;nbsp;have to say about us</font></strong>
                                </h2>
                                <ol class="text-center small list-unstyled">
                                    <li>Explore the testimonials of our satisfied customers and witness the</li>
                                    <li>peace of mind that comes with choosing QuickShip for your car shipping</li>
                                    <li>needs to the breathtaking landscapes of Hawaii.<br/></li>
                                </ol>
                            </div>
                        </section>

                    </div>
                </t>
            </t>

        </field>
    </record>

</odoo>
